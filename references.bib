author = {Bordin, Andr{\'{e}}a Sabedra and Gon{\c{c}}alves, Alexandre Leopoldo and Todesco, Jos{\'{e}} Leomar},
doi = {10.1590/1981-5344/1796},
issn = {1413-9936},
journal = {Perspectivas em Ci{\^{e}}ncia da Informa{\c{c}}{\~{a}}o},
number = {2},
pages = {37--52},
title = {{An{\'{a}}lise da colabora{\c{c}}{\~{a}}o cient{\'{i}}fica departamental atrav{\'{e}}s de redes de coautoria}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S1413-99362014000200004{\&}lng=pt{\&}tlng=pt},
