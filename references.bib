year = {2008}
}

@article{Silva2006,
abstract = {{\textless}p{\textgreater}Este artigo apresenta a An{\'{a}}lise de Redes Sociais (ARS) como um m{\'{e}}todo a ser aplicado em estudos na ci{\^{e}}ncia da informa{\c{c}}{\~{a}}o (CI), inclusive para a compreens{\~{a}}o da estrutura{\c{c}}{\~{a}}o da pesquisa nessa {\'{a}}rea do conhecimento. Esta {\'{e}}, normalmente, apresentada como uma {\'{a}}rea do conhecimento interdisciplinar, e as diferentes linhas de pesquisa existentes nos Programas de P{\'{o}}s-Gradua{\c{c}}{\~{a}}o, no Brasil, recebem influ{\^{e}}ncias de diferentes {\'{a}}reas do conhecimento. A aplica{\c{c}}{\~{a}}o da ARS no estudo da rede de co-autoria dos professores do PPGCI/UFMG permite tanto a apresenta{\c{c}}{\~{a}}o do m{\'{e}}todo, quanto a obten{\c{c}}{\~{a}}o de resultados emp{\'{i}}ricos para alimentar a discuss{\~{a}}o sobre a CI. As redes de colabora{\c{c}}{\~{a}}o entre os professores refletem, em tese, a participa{\c{c}}{\~{a}}o em programas de pesquisa da {\'{a}}rea. A colabora{\c{c}}{\~{a}}o entre professores das diferentes linhas se relaciona com a integra{\c{c}}{\~{a}}o das diferentes interdisciplinaridades que elas apresentam. O artigo exp{\~{o}}e, inicialmente, uma discuss{\~{a}}o sobre a interdisciplinaridade na CI; em seguida d{\'{a}} uma vis{\~{a}}o geral da ARS e apresenta estudos da {\'{a}}rea, destacando-se a an{\'{a}}lise de redes de co-autoria. Finalmente, descreve-se a metodologia da pesquisa e os principais resultados obtidos da ARS. A conclus{\~{a}}o destaca os resultados e refor{\c{c}}a a import{\^{a}}ncia da ARS como m{\'{e}}todo para a CI, o qual pode ser explorado com sucesso, inclusive em pesquisas sobre redes de co-autoria e interdisciplinaridade.{\textless}/p{\textgreater}},
author = {Silva, Antonio Braz de Oliveira e and Matheus, Renato Fabiano and Parreiras, Fernando Silva and Parreiras, Tatiane A. Silva},
doi = {10.1590/S0100-19652006000100009},
issn = {0100-1965},
journal = {Ci{\^{e}}ncia da Informa{\c{c}}{\~{a}}o},
month = {apr},
number = {1},
pages = {72--93},
publisher = {IBICT},
title = {{An{\'{a}}lise de redes sociais como metodologia de apoio para a discuss{\~{a}}o da interdisciplinaridade na ci{\^{e}}ncia da informa{\c{c}}{\~{a}}o}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0100-19652006000100009{\&}lng=pt{\&}tlng=pt},
